<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     >
<LinearLayout
    android:id="@+id/home_parts1_root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="22dp"
    android:paddingLeft="3dp"
    android:gravity="top"
    android:paddingRight="3dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/home_parts"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:src="@drawable/home_parts_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/four_font"
                android:text="名牌配件"
                android:layout_marginTop="10dp"
                android:textColor="#5F5D5D"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/home_support"
                android:layout_width="match_parent"
                android:clickable="true"
                android:layout_height="wrap_content"
                android:src="@drawable/home_support_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/four_font"
                android:text="养修"
                android:layout_marginTop="10dp"
                android:textColor="#5F5D5D"/>
        </LinearLayout>



        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/home_beauty"
                android:layout_width="match_parent"
                android:clickable="true"
                android:layout_height="wrap_content"
                android:src="@drawable/home_beauty_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/four_font"
                android:text="美容"
                android:layout_marginTop="10dp"
                android:textColor="#5F5D5D"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            >
            <ImageView
                android:id="@+id/home_gps"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:src="@drawable/home_gps_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/four_font"
                android:text="导航"
                android:layout_marginTop="10dp"
                android:textColor="#5F5D5D"/>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:orientation="horizontal">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/home_earnings"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:src="@drawable/home_earnings_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/four_font"
                android:text="原厂汽配"
                android:layout_marginTop="10dp"
                android:textColor="#5F5D5D"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/home_ask"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:src="@drawable/home_ask_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/four_font"
                android:text="收益"
                android:layout_marginTop="10dp"
                android:textColor="#5F5D5D"/>
        </LinearLayout>



        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical">
            <ImageView
                android:id="@+id/home_brake"
                android:layout_width="match_parent"
                android:clickable="true"
                android:layout_height="wrap_content"
                android:src="@drawable/home_brake_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/four_font"
                android:text="刹车片"
                android:layout_marginTop="10dp"
                android:textColor="#5F5D5D"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            >
            <ImageView
                android:id="@+id/home_tire"
                android:layout_width="match_parent"
                android:clickable="true"
                android:layout_height="wrap_content"
                android:src="@drawable/home_tire_btn"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/four_font"
                android:text="轮胎"
                android:layout_marginTop="10dp"
                android:textColor="#5F5D5D"/>
        </LinearLayout>
    </LinearLayout>


</LinearLayout>
    </RelativeLayout>